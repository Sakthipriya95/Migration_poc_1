<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="PIDCTreeViewExtnPt" name="PIDCTreeViewExtnPt" schema="schema/PIDCTreeViewExtnPt.exsd"/>
   <extension-point id="UseCaseTreeViewExtnPt" name="UseCaseTreeViewExtnPt" schema="schema/UseCaseTreeViewExtnPt.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.ui"
            class="org.eclipse.ui.ExtensionFactory:progressView"
            icon="icons/pview.gif"
            id="org.eclipse.ui.views.ProgressView"
            name="Progress View"
            restorable="true">
      </view>
      <view
            category="com.bosch.caltool.icdm.ui.icdm.category"
            class="com.bosch.caltool.icdm.common.ui.views.SeriesStatisticsViewPart"
            icon="icons/series_obj.gif"
            id="com.bosch.caltool.icdm.common.ui.views.SeriesStatisticsViewPart"
            name="SeriesStatistics"
            restorable="true">
      </view>
      <view
            category="com.bosch.caltool.icdm.ui.icdm.category"
            class="com.bosch.caltool.icdm.common.ui.views.FCBCUsageViewPart"
            icon="icons/fc_bc_usage.gif"
            id="com.bosch.caltool.icdm.common.ui.views.FCBCUsageViewPart"
            name="FC/BC Usage"
            restorable="true">
      </view>
      <view
            category="org.eclipse.ui"
            class="com.bosch.caltool.icdm.common.ui.views.TransactionSummaryViewPart"
            icon="icons/summary.png"
            id="com.bosch.caltool.icdm.common.ui.views.TransactionSummaryViewPart"
            name="Transaction Summary"
            restorable="true">
      </view>
   </extension>
<extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.A2LFile"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.IPIDCAttribute"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.NodeAccessRight"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.Attribute"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.PIDCard"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.PIDCVariant"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
        <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.PIDCDetailsNode"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.PIDCSubVariant"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
        
      
     
      <factory
            adaptableType="com.bosch.caltool.icdm.client.bo.uc.IUseCaseItemClientBO"
            class="com.bosch.caltool.icdm.common.ui.views.providers.UseCaseItemPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
       <factory
            adaptableType="com.bosch.caltool.icdm.client.bo.uc.FavUseCaseItemNode"
            class="com.bosch.caltool.icdm.common.ui.views.providers.UseCaseItemPropertiesViewAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.bosch.caltool.apic.jpa.bo.UseCaseNatInput"
            class="com.bosch.caltool.icdm.common.ui.views.providers.APICPropertiesViewAdapterFactory">
